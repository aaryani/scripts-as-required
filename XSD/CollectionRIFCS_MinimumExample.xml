<?xml version="1.0" encoding="UTF-8"?>
<registryObjects xmlns:xs="http://www.w3.org/2001/XMLSchema"
    xmlns:custom="http://custom.nowhere.yet"
    xmlns:mcp="http://bluenet3.antcrc.utas.edu.au/mcp"
    xmlns="http://ands.org.au/standards/rif-cs/registryObjects"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://ands.org.au/standards/rif-cs/registryObjects ./registryObjects_modified.xsd">
    <registryObject group="registryGroup">
        <key>Possibly Automatically generated by tool, unique</key>
        <originatingSource>?</originatingSource>
    <collection type="collectionType">
        <name>
            <namePart>Canonical name (more than one name possible)</namePart>
        </name>
        <description type="full">Multiple (long/short) descriptions? </description>
        <location>
            <address>
                <electronic type="uri" target="landingPage">
                    <value>http://metadata.point.of.truth</value>
                    <mediaType></mediaType>
                </electronic>
                <electronic type='uri' target='directDownload'>
                    <value>http://www.provider.au/direct.data.download.zip</value>
                    <mediaType>application/zip</mediaType>
                    <title></title>
                    <notes></notes>
                    
                </electronic>
            </address>
        </location>
       <subject type="anzsrc-for"></subject>
        <subject type="local"></subject>
        <coverage>
            <spatial type="gmlPolyCoords"></spatial>
            <temporal><date type="" dateFormat=""></date></temporal>
        </coverage>
        <relatedInfo type="party">
            <identifier type="orcid">orcid.value.of.principalInvestigator</identifier>
            <relation type="hasPrincipalInvestigator"/>
        </relatedInfo>
        <relatedInfo type="collection">
            <identifier type="doi">doi.to.related.collection</identifier>
            <relation type="hasPart"></relation>
        </relatedInfo>
        <!-- Should be mandatory if funded by ARC or NHMRC -->
        <relatedInfo type="activity">
            <identifier type="uri">grant.id.uri</identifier>
            <relation type="isOutputOf"></relation>
        </relatedInfo>
        <relatedInfo type="reuseInformation">
            <identifier type="uri">reuse.information.uri</identifier>
        </relatedInfo>
        <rights>
            <rightsStatement>A free text description of the access restrictions on the data, e.g. 
                ‘To access data, you must submit an ethics application to ...’ or 
                ‘contact the custodian to negotiate access to this collection’ </rightsStatement>
            <licence type="CC-BY">Either select a licence or explicitly state ‘no licence’ </licence>
            <accessRights type="open"></accessRights>
        </rights>
        <!-- Schema validation requires either fullCitation OR citationMetadata -->
        <citationInfo>
            <fullCitation>Could be automated from other fields: 
                “Custodian, (year from latest Date?), Title. Electronic dataset, 
                Institution, metadata location. 
            </fullCitation>
        </citationInfo>
        <citationInfo>        
            <citationMetadata>
                <identifier type="doi">doi.to.landing.page</identifier>
                <contributor seq="1">
                    <namePart>Surname,Firstname,Title</namePart>
                </contributor>
                <contributor seq="2">
                    <namePart>Surname,Firstname,Title</namePart>
                </contributor>
                <title>Canonical name</title>
                <publisher></publisher>
                <date type=""></date>
            </citationMetadata>
        </citationInfo>
    </collection>
        
    </registryObject>
</registryObjects>

